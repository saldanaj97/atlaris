NODE_ENV=development
MAINTENANCE_MODE=false

# Local Dev Database
DATABASE_URL=
DATABASE_URL_UNPOOLED=
DATABASE_URL_STAGING=
DATABASE_URL_TESTING=

# Vercel
VERCEL_TOKEN=
VERCEL_ORG_ID=
VERCEL_PROJECT_ID=

# Sentry
SENTRY_AUTH_TOKEN=

# Neon Auth
# Branch auth URL from Neon Console -> Branch -> Auth -> Configuration
NEON_AUTH_BASE_URL=

# Must be at least 32 chars. Generate with: openssl rand -base64 32
NEON_AUTH_COOKIE_SECRET=

# Turbo Remote Caching
TURBO_TOKEN=
TURBO_TEAM=

# Stripe API Keys
STRIPE_SECRET_KEY=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=

# Stripe Webhook
STRIPE_WEBHOOK_SECRET=

# Stripe Product/Price IDs (create these in Dashboard first)
STRIPE_STARTER_MONTHLY_PRICE_ID=
STRIPE_STARTER_YEARLY_PRICE_ID=
STRIPE_PRO_MONTHLY_PRICE_ID=
STRIPE_PRO_YEARLY_PRICE_ID=

# Core AI Routing
AI_PROVIDER=router
AI_USE_MOCK=false
AI_MAX_OUTPUT_TOKENS=2000
AI_DEFAULT_MODEL=google/gemini-2.0-flash-exp:free
AI_PRIMARY=gemini-2.0-flash-lite
AI_FALLBACK="openai/gpt-oss-20b:free"
MOCK_GENERATION_DELAY_MS=2000
MOCK_GENERATION_FAILURE_RATE=0.1

# Google AI Studio
GOOGLE_GENERATIVE_AI_API_KEY=

# OpenRouter (only if enabling overflow/paid)
OPENROUTER_API_KEY=
OPENROUTER_SITE_URL=http://localhost:3000
OPENROUTER_APP_NAME=

# Dev User
DEV_AUTH_USER_ID=
DEV_AUTH_USER_EMAIL=
DEV_AUTH_USER_NAME=

# Plan Generation Polling
# Use shorter times for quick development and testing
NEXT_PLAN_GENERATION_POLL_INTERVAL_MS=5000
NEXT_PLAN_GENERATION_MAX_POLLS=3

# Youtube
YOUTUBE_API_KEY=

# Google Caldner
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=

# OAuth Encryption key
OAUTH_ENCRYPTION_KEY=
