{
  "name": "atlaris",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build --turbopack",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:test-db": "drizzle-kit migrate --config drizzle-test.config.ts",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:push:test-db": "drizzle-kit push --config drizzle-test.config.ts",
    "dev": "next dev --turbopack",
    "dev:stripe": "stripe listen --skip-verify --forward-to http://localhost:3000/api/v1/stripe/webhook",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "lint": "NODE_OPTIONS='--max-old-space-size=8192' eslint . --ext .ts,.tsx",
    "lint:fix": "NODE_OPTIONS='--max-old-space-size=8192' eslint . --ext .ts,.tsx --fix",
    "seed": "tsx src/lib/db/seed-cli.ts development",
    "seed:refresh": "pnpm seed:reset && pnpm seed",
    "seed:reset": "tsx src/lib/db/seed-cli.ts reset",
    "test": "vitest run",
    "test:unit:full": "vitest run --project unit tests/unit",
    "test:unit:fast": "SKIP_DB_TEST_SETUP=true vitest run --project unit tests/unit",
    "test:unit:related": "vitest related --project unit --run",
    "test:integration:full": "bash scripts/test-with-db.sh tests/integration",
    "test:integration:related": "bash scripts/test-with-db.sh tests/integration -- --related",
    "test:e2e:full": "bash scripts/test-with-db.sh tests/e2e",
    "test:e2e:related": "bash scripts/test-with-db.sh tests/e2e -- --related",
    "test:rls:full": "bash scripts/test-with-db.sh tests/security",
    "test:rls:related": "bash scripts/test-with-db.sh tests/security -- --related",
    "test:watch": "vitest",
    "test:suite:full": "bash scripts/full-test-suite.sh",
    "test:suite:all": "bash scripts/full-test-suite.sh --with-e2e",
    "test:local:integration": "docker-compose -f docker-compose.test.yml up -d && sleep 3 && USE_LOCAL_NEON=true pnpm test:integration:full; docker-compose -f docker-compose.test.yml down",
    "test:local:e2e": "docker-compose -f docker-compose.test.yml up -d && sleep 3 && USE_LOCAL_NEON=true pnpm test:e2e:full; docker-compose -f docker-compose.test.yml down",
    "test:local": "docker-compose -f docker-compose.test.yml up -d && sleep 3 && USE_LOCAL_NEON=true pnpm test:suite:full; docker-compose -f docker-compose.test.yml down",
    "test:smoke": "vitest run tests/smoke --reporter=verbose",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "check:duplication": "jscpd --exitCode 0",
    "check:quality": "pnpm check:duplication",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.20",
    "@ai-sdk/openai": "^2.0.48",
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@clerk/nextjs": "^6.31.9",
    "@neondatabase/serverless": "^1.0.2",
    "@notionhq/client": "^5.3.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.27.0",
    "ai": "^5.0.68",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "drizzle-seed": "^0.3.1",
    "googleapis": "^164.1.0",
    "import-in-the-middle": "^2.0.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.542.0",
    "nanoid": "^5.1.6",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "p-retry": "^7.1.0",
    "pino": "^10.1.0",
    "postgres": "^3.4.7",
    "react": "19.2.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.1",
    "require-in-the-middle": "^8.0.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@next/eslint-plugin-next": "^15",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "async-mutex": "^0.5.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^5",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "typescript-eslint": "^8.43.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.2"
  }
}
